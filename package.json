{
  "name": "bookstore-sample-app",
  "version": "2.2.8",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "xi18n": "ng extract-i18n",
    "deploy": "npm run build && firebase deploy",
    "lint": "stylelint \"**/*.scss\" && ng lint && prettier . --check",
    "fix": "stylelint \"**/*.scss\" --fix && ng lint --fix && prettier . --write",
    "browsers": "browserslist && browserslist --coverage",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/bookstore-sample-app/en/stats.json",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.4",
    "@angular/cdk": "^20.0.3",
    "@angular/common": "^20.0.4",
    "@angular/compiler": "^20.0.4",
    "@angular/core": "^20.0.4",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.0.4",
    "@angular/material": "^20.0.3",
    "@angular/platform-browser": "^20.0.4",
    "@angular/platform-browser-dynamic": "^20.0.4",
    "@angular/router": "^20.0.4",
    "@ngrx/effects": "^19.0.1",
    "@ngrx/entity": "^19.0.1",
    "@ngrx/operators": "^19.0.1",
    "@ngrx/router-store": "^19.0.1",
    "@ngrx/store": "^19.0.1",
    "@ngrx/store-devtools": "^19.0.1",
    "firebase": "^11.6.0",
    "flagpack": "^1.0.5",
    "ngx-image-cropper": "^9.1.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.3",
    "@angular/cli": "^20.0.3",
    "@angular/compiler-cli": "^20.0.4",
    "@angular/localize": "^20.0.4",
    "@ngrx/eslint-plugin": "^19.0.1",
    "@ngrx/schematics": "^19.0.1",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/node": "^24.0.3",
    "angular-eslint": "^20.1.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-regexp": "^2.1.1",
    "eslint-plugin-rxjs-x": "^0.7.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.0.11",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-merge": "^0.7.3",
    "prettier-plugin-multiline-arrays": "^4.0.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "stylelint": "^16.18.0",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-scss": "^15.0.1",
    "tailwindcss": "^4.1.3",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.33.1",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
