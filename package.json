{
  "name": "bookstore-sample-app",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "xi18n": "ng extract-i18n",
    "deploy": "ng deploy",
    "lint": "ng lint --fix",
    "lint:check": "ng lint",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "browsers": "browserslist --coverage && browserslist",
    "browsers:update": "browserslist --update-db",
    "analyze": "ng build --stats-json&&webpack-bundle-analyzer dist/bookstore-sample-app/stats.json",
    "prepare": "husky install &"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.3",
    "@angular/cdk": "^16.2.12",
    "@angular/common": "^16.1.3",
    "@angular/compiler": "^16.1.3",
    "@angular/core": "^16.1.3",
    "@angular/fire": "^7.6.1",
    "@angular/forms": "^16.1.3",
    "@angular/material": "^16.2.12",
    "@angular/platform-browser": "^16.1.3",
    "@angular/platform-browser-dynamic": "^16.1.3",
    "@angular/router": "^16.1.3",
    "@ngrx/component": "^16.0.1",
    "@ngrx/effects": "^16.0.1",
    "@ngrx/entity": "^16.0.1",
    "@ngrx/router-store": "^16.0.1",
    "@ngrx/store": "^16.0.1",
    "@ngrx/store-devtools": "^16.0.1",
    "bootstrap": "^5.2.3",
    "flagpack": "^1.0.5",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "ngx-image-cropper": "^7.1.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.10",
    "@angular-eslint/builder": "^16.3.1",
    "@angular-eslint/eslint-plugin": "^16.3.1",
    "@angular-eslint/eslint-plugin-template": "^16.3.1",
    "@angular-eslint/schematics": "^16.3.1",
    "@angular-eslint/template-parser": "^16.3.1",
    "@angular/cli": "~16.2.10",
    "@angular/compiler-cli": "^16.1.3",
    "@angular/localize": "^16.1.3",
    "@ngrx/eslint-plugin": "^16.0.1",
    "@ngrx/schematics": "^16.0.1",
    "@types/node": "^20.9.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-regexp": "^2.1.1",
    "eslint-plugin-rxjs": "^5.0.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "typescript": "~4.9.5",
    "webpack-bundle-analyzer": "^4.10.1"
  }
