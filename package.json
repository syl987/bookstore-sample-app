{
  "name": "silly-bookstore-example-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "xi18n": "ng extract-i18n",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "browserslist": "browserslist --coverage && browserslist",
    "browserslist:update": "browserslist --update-db",
    "analyze": "ng build --stats-json&&webpack-bundle-analyzer dist/silly-bookstore-example-app/stats.json",
    "prepare": "husky install &"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.5",
    "@angular/cdk": "^15.2.5",
    "@angular/common": "^15.2.5",
    "@angular/compiler": "^15.2.5",
    "@angular/core": "^15.2.5",
    "@angular/fire": "^7.5.0",
    "@angular/forms": "^15.2.5",
    "@angular/material": "^15.2.5",
    "@angular/platform-browser": "^15.2.5",
    "@angular/platform-browser-dynamic": "^15.2.5",
    "@angular/router": "^15.2.5",
    "@ngrx/data": "^15.4.0",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/entity": "^15.4.0",
    "@ngrx/router-store": "^15.4.0",
    "@ngrx/store": "^15.4.0",
    "@ngrx/store-devtools": "^15.4.0",
    "base64-blob": "^1.4.1",
    "bootstrap": "^5.2.3",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "ngx-image-cropper": "^6.3.2",
    "normalize.css": "^8.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.5",
    "@angular/language-service": "^15.2.5",
    "@angular/localize": "^15.2.5",
    "@ngrx/schematics": "^15.4.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "typescript": "~4.9.5",
    "webpack-bundle-analyzer": "^4.8.0"
  }
}
