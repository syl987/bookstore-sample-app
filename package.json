        "@angular/core": "^15.2.5",
        "@angular/fire": "^7.4.1",
        "@angular/forms": "^15.2.5",
        "@angular/material": "^15.2.5",
        "@angular/platform-browser": "^15.2.5",
        "@angular/platform-browser-dynamic": "^15.2.5",
        "@angular/router": "^15.2.5",
        "@ngrx/data": "^15.4.0",
        "@ngrx/effects": "^15.4.0",
        "@ngrx/entity": "^15.4.0",
        "@ngrx/router-store": "^15.4.0",
        "@ngrx/store": "^15.4.0",
        "@ngrx/store-devtools": "^15.4.0",
        "base64-blob": "^1.4.1",
