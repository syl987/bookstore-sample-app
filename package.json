        "eslint-plugin-deprecation": "~1.3.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-regexp": "^1.9.0",
        "eslint-plugin-rxjs": "^5.0.2",
        "eslint-plugin-simple-import-sort": "~7.0.0",
        "eslint-plugin-unused-imports": "~2.0.0",
        "husky": "^8.0.1",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "typescript": "~4.9.5",
