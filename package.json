{
  "name": "bookstore-sample-app",
  "version": "1.4.6",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "xi18n": "ng extract-i18n",
    "deploy": "npm run build && firebase deploy",
    "lint": "ng lint --fix",
    "lint:check": "ng lint",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "browsers": "browserslist --coverage && browserslist",
    "browsers:update": "browserslist --update-db",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/bookstore-sample-app/en/stats.json",
    "prepare": "husky install &"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.4",
    "@angular/cdk": "^17.0.1",
    "@angular/common": "^17.0.4",
    "@angular/compiler": "^17.0.4",
    "@angular/core": "^17.0.4",
    "@angular/fire": "^17.0.0-next.0",
    "@angular/forms": "^17.0.4",
    "@angular/material": "^17.0.1",
    "@angular/platform-browser": "^17.0.4",
    "@angular/platform-browser-dynamic": "^17.0.4",
    "@angular/router": "^17.0.4",
    "@ngrx/component": "^17.0.0",
    "@ngrx/effects": "^17.0.0",
    "@ngrx/entity": "^17.0.0",
    "@ngrx/router-store": "^17.0.0",
    "@ngrx/store": "^17.0.0",
    "@ngrx/store-devtools": "^17.0.0",
    "bootstrap": "~5.2.3",
    "flagpack": "^1.0.5",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "ngx-image-cropper": "^7.1.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular-eslint/builder": "^17.1.0",
    "@angular-eslint/eslint-plugin": "^17.1.0",
    "@angular-eslint/eslint-plugin-template": "^17.1.0",
    "@angular-eslint/schematics": "^17.1.0",
    "@angular-eslint/template-parser": "^17.1.0",
    "@angular/cli": "^17.0.3",
    "@angular/compiler-cli": "^17.0.4",
    "@angular/localize": "^17.0.4",
    "@ngrx/eslint-plugin": "^17.0.0",
    "@ngrx/schematics": "^17.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-regexp": "^2.1.1",
    "eslint-plugin-rxjs": "^5.0.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "typescript": "~5.2.2",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
