{
    "name": "silly-bookstore-example-app",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "xi18n": "ng extract-i18n",
        "lint": "ng lint",
        "lint:fix": "ng lint --fix",
        "format": "prettier . --check",
        "format:fix": "prettier . --write",
        "browserslist": "browserslist --coverage && browserslist",
        "browserslist:update": "browserslist --update-db",
        "analyze": "ng build --stats-json&&webpack-bundle-analyzer dist/silly-bookstore-example-app/stats.json",
        "prepare": "husky install &"
    },
    "private": true,
    "dependencies": {
