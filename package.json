{
  "name": "bookstore-sample-app",
  "version": "1.4.25",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "xi18n": "ng extract-i18n",
    "deploy": "npm run build && firebase deploy",
    "lint": "ng lint",
    "format": "prettier . --check",
    "fix": "ng lint --fix && prettier . --write",
    "browsers": "browserslist --coverage && browserslist",
    "browsers:update": "browserslist --update-db",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/bookstore-sample-app/en/stats.json",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.4",
    "@angular/cdk": "^19.2.7",
    "@angular/common": "^19.2.4",
    "@angular/compiler": "^19.2.4",
    "@angular/core": "^19.2.4",
    "@angular/fire": "^19.0.0",
    "@angular/forms": "^19.2.4",
    "@angular/material": "^19.2.7",
    "@angular/platform-browser": "^19.2.4",
    "@angular/platform-browser-dynamic": "^19.2.4",
    "@angular/router": "^19.2.4",
    "@ngrx/effects": "^19.0.1",
    "@ngrx/entity": "^19.0.1",
    "@ngrx/router-store": "^19.0.1",
    "@ngrx/store": "^19.0.1",
    "@ngrx/store-devtools": "^19.0.1",
    "bootstrap": "~5.3.2",
    "flagpack": "^1.0.5",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "ngx-image-cropper": "^8.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^19.3.0",
    "@angular-eslint/eslint-plugin": "^19.3.0",
    "@angular-eslint/eslint-plugin-template": "^19.3.0",
    "@angular-eslint/schematics": "^19.3.0",
    "@angular-eslint/template-parser": "^19.3.0",
    "@angular/build": "^19.2.5",
    "@angular/cli": "^19.2.5",
    "@angular/compiler-cli": "^19.2.4",
    "@angular/localize": "^19.2.4",
    "@ngrx/eslint-plugin": "^19.0.1",
    "@ngrx/schematics": "^19.0.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-regexp": "^2.1.1",
    "eslint-plugin-rxjs": "^5.0.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "typescript": "~5.8.2",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
