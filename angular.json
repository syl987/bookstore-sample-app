{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "bookstore-sample-app": {
      "i18n": {
        "sourceLocale": {
          "code": "en"
        },
        "locales": {
          "de": "src/locale/messages.de.json",
          "es": "src/locale/messages.es.json"
        }
      },
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/bookstore-sample-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": ["hammerjs"],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": ["src/styles/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["node_modules/", "src/styles/shared"]
            },
            "allowedCommonJsDependencies": ["b64-to-blob"],
            "fileReplacements": [
              {
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.any.ts"
              }
            ],
            "i18nDuplicateTranslation": "error",
            "i18nMissingTranslation": "error",
            "localize": true,
            "scripts": []
          },
          "configurations": {
            "serve": {
              "localize": ["de"]
            },
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "polyfills": ["hammerjs", "zone.js/plugins/zone-error"],
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "bookstore-sample-app:build:production,serve"
            },
            "development": {
              "buildTarget": "bookstore-sample-app:build:development,serve"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "format": "json",
            "outputPath": "src/locale",
            "buildTarget": "bookstore-sample-app:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "skipTests": true,
      "standalone": true,
      "changeDetection": "OnPush",
      "inlineStyle": true,
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "standalone": true,
      "skipTests": true
    },
    "@schematics/angular:pipe": {
      "standalone": true,
      "skipTests": true
    },
    "@schematics/angular:service": {
      "skipTests": true
    },
    "@schematics/angular:resolver": {
      "skipTests": true
    },
    "@schematics/angular:guard": {
      "skipTests": true
    },
    "@schematics/angular:interceptor": {
      "skipTests": true
    },
    "@schematics/angular:module": {
      "commonModule": false
    },
    "@schematics/angular:class": {
      "skipTests": true,
      "type": "models"
    },
    "@schematics/angular:enum": {
      "type": "models"
    },
    "@schematics/angular:interface": {
      "type": "models"
    },
    "@ngrx/schematics:store": {
      "statePath": "store",
      "stateInterface": "State"
    },
    "@ngrx/schematics:action": {
      "creators": true,
      "flat": false,
      "api": true
    },
    "@ngrx/schematics:reducer": {
      "skipTests": true,
      "creators": true,
      "flat": false,
      "api": true
    },
    "@ngrx/schematics:effect": {
      "skipTests": true,
      "creators": false,
      "flat": false,
      "api": true
    },
    "@ngrx/schematics:selector": {
      "skipTests": true,
      "creators": true,
      "flat": false,
      "api": true
    },
    "@ngrx/schematics:entity": {
      "skipTests": true,
      "creators": true,
      "flat": false
    },
    "@ngrx/schematics:feature": {
      "skipTests": true,
      "creators": true,
      "flat": false,
      "api": true
    },
    "@ngrx/schematics:container": {
      "testDepth": "unit",
      "changeDetection": "OnPush",
      "inlineStyle": true,
      "style": "scss"
    },
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    }
  },
  "cli": {
    "schematicCollections": ["@angular-eslint/schematics", "@ngrx/schematics", "@schematics/angular"],
    "packageManager": "npm",
    "analytics": false
  }
}
