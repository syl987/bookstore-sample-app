{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "bookstore-sample-app": {
      "i18n": {
        "sourceLocale": { "code": "en" },
        "locales": {
          "de": "src/locales/messages.de.json"
        }
      },
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/bookstore-sample-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": ["zone.js", "hammerjs"],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["node_modules/font-awesome/css/font-awesome.css", "src/styles/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["node_modules/", "src/styles/shared"]
            },
            "allowedCommonJsDependencies": ["b64-to-blob"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "localize": true,
              "i18nMissingTranslation": "error",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "polyfills": ["zone.js", "hammerjs", "zone.js/plugins/zone-error"],
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "bookstore-sample-app:build:production"
            },
            "development": {
              "browserTarget": "bookstore-sample-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "format": "json",
            "outputPath": "src/locales",
            "browserTarget": "bookstore-sample-app:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {
            "prerender": false,
            "ssr": false,
            "browserTarget": "bookstore-sample-app:build:production",
            "firebaseProject": "sample-app-a00e0",
            "firebaseHostingSite": "sample-app-a00e0"
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "standalone": true,
      "changeDetection": "OnPush",
      "inlineStyle": true,
      "skipTests": true,
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "standalone": true,
      "skipTests": true
    },
    "@schematics/angular:pipe": {
      "standalone": true,
      "skipTests": true
    },
    "@schematics/angular:service": {
      "skipTests": true
    },
    "@schematics/angular:interceptor": {
      "skipTests": true
    },
    "@schematics/angular:resolver": {
      "skipTests": true
    },
    "@schematics/angular:guard": {
      "skipTests": true
    },
    "@schematics/angular:class": {
      "skipTests": true,
      "type": "models"
    },
    "@schematics/angular:enum": {
      "type": "models"
    },
    "@schematics/angular:interface": {
      "type": "models"
    },
    "@ngrx/schematics:container": {
      "changeDetection": "OnPush",
      "inlineStyle": true,
      "style": "scss",
      "lintFix": true
    },
    "@ngrx/schematics:store": {
      "statePath": "store",
      "stateInterface": "State"
    },
    "@ngrx/schematics:action": {
      "skipTest": true,
      "creators": true,
      "flat": false,
      "api": true
    },
    "@ngrx/schematics:reducer": {
      "creators": true,
      "flat": false,
      "api": true,
      "skipTests": true
    },
    "@ngrx/schematics:effect": {
      "creators": false,
      "flat": false,
      "api": true,
      "skipTests": true
    },
    "@ngrx/schematics:selector": {
      "creators": true,
      "flat": false,
      "api": true,
      "spec": false
    },
    "@ngrx/schematics:entity": {
      "creators": true,
      "flat": false,
      "skipTests": true
    },
    "@ngrx/schematics:feature": {
      "creators": true,
      "flat": false,
      "api": true,
      "skipTests": true
    },
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    }
  },
  "cli": {
    "schematicCollections": ["@angular-eslint/schematics", "@ngrx/schematics", "@schematics/angular"],
    "packageManager": "npm",
    "analytics": false
  }
}
